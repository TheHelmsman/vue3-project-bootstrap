{
  "name": "vue3-project-bootstrap",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "yarn run check-build && yarn run test:unit && vite build",
    "dev": "vite build --mode development",
    "test:unit": "jest --passWithNoTests",
    "test:unit:watch": "jest --watchAll",
    "check-all": "yarn run tsc && yarn run lint && yarn run stylelint",
    "check-build": "yarn run tsc",
    "tsc": "vue-tsc --noEmit",
    "lint": "eslint --ext .ts,.vue --fix tests src",
    "stylelint": "stylelint **/*.{scss,vue} --fix"
  },
  "dependencies": {
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.29",
    "vite-plugin-vue-type-imports": "^0.2.4",
    "vue": "^3.2.6",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@pinia/testing": "0.0.14",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.3.2",
    "@vue/vue3-jest": "^29.2.5",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-kms": "^1.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-modules-newline": "0.0.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.12.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jsdom": "^20.0.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "rollup-plugin-iconfont": "^1.0.3",
    "rollup-plugin-visualizer": "^5.8.0",
    "sass": "^1.58.0",
    "style-resources-loader": "^1.5.0",
    "stylelint": "^14.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "~4.9.5",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  }
}
